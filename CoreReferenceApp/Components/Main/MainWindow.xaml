<Window x:Class="CoreReferenceApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoreReferenceApp"
        mc:Ignorable="d" MinHeight="200" MinWidth="200"
        Title="{Binding Path=DataContext.Title, RelativeSource={RelativeSource Self}}" 
        Height="{Binding Path=DataContext.Height, RelativeSource={RelativeSource Self}}"
        Width="{Binding Path=DataContext.Width, RelativeSource={RelativeSource Self}}" 
        Top="{Binding Path=DataContext.Top, RelativeSource={RelativeSource Self}}" 
        Left="{Binding Path=DataContext.Left, RelativeSource={RelativeSource Self}}">
    <Grid>
        
    </Grid>
</Window>
